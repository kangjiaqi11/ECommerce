                            <div class="btn-group" data-toggle="buttons">
                                <label class="btn blue active">
                                    <input type="radio" class="toggle"><trans oldtip=" Option 1 " onclick="Show(0)" newtip="备选案文1" style="">全部订单</trans>
                                </label>
                                <label class="btn blue">
                                    <input type="radio" class="toggle"><trans oldtip=" Option 2 " onclick="Show(1)" newtip="备选案文2" style="">待付款</trans>
                                </label>
                                <label class="btn blue">
                                    <input type="radio" class="toggle"><trans oldtip=" Option 3 " newtip="备选方案3" onclick="Show(2)" style="">待发货</trans>
                                </label>
                                <label class="btn blue">
                                    <input type="radio" class="toggle"><trans oldtip=" Option 3 " newtip="备选方案3" onclick="Show(3)" style="">已发货</trans>
                                </label>
                                <label class="btn blue">
                                    <input type="radio" class="toggle"><trans oldtip=" Option 3 " newtip="备选方案3" onclick="Show(4)" style="">已完成</trans>
                                </label>
                                <label class="btn blue">
                                    <input type="radio" class="toggle"><trans oldtip=" Option 3 " newtip="备选方案3" onclick="Show(5)" style="">已关闭</trans>
                                </label>
                            </div>
