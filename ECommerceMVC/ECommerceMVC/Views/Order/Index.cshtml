    <script>
        Show();
    function Show(State) {
       
            var OrderNumberGoodsName = $("#OrderNumberGoodsName").val();
            var AddrNameAddrPhone = $("#AddrNameAddrPhone").val();
             var times = $("#times").val();
                $("#ActivityShow").html("");
                $.ajax({
                url: "Index",
                    type: "POST",
                    data: { OrderNumberGoodsName: OrderNumberGoodsName, AddrNameAddrPhone: AddrNameAddrPhone, State: State,puttime: times },
                dataType: "json",
                    success: function (res) {
                     
                               $(res).each(function () {
                        var tr = '<tr>'
                            + '<th>'+ this.OrderNumber + '</th>'
                            + '<th>' + this.SubmitTimeST + '</th>'
                            + '<th>' + this.Account + '</th>'
                            + '<th>' + this.MarketPrice + '</th>'
                            + '<th>' + this.payment + '</th>'
                            + '<th>' + this.Ordersource + '</th>'
                            + '<th>' + (this.Oderstate==1?"待付款":this.Oderstate==2?"待发货":this.Oderstate==3?"待收货":this.Oderstate==4?"已完成":"已关闭")+'</th>'
                            + '<th> <a onclick="shen('+this.OrderNumber+')";>查看申请</a>  <a onclick="Edit('+this.OrderId+','+this.Oderstate+')">'+(this.Oderstate==1?"关闭订单":this.Oderstate==2?"订单发货":this.Oderstate==3?"订单跟踪":this.Oderstate==4?"订单跟踪":"删除订单")+'</a></th>'
                            + '</tr> '

                        
                        $("#ActivityShow").append(tr);
                    })
                }
                    
                 
            });
            }
            
     
        function Edit(id, state) {
            if (state == 1) {
                $.ajax({
                    url:"UpdateState",
                    type: "POST",
                    data: { id: id },
                    dataType: "json",
                    success: function (res) {
                        alert(res.Msg)
                    }
                    
                })
            }
            if (state == 2) {
                location.href='/Order/expressShow'
            }
           
            
            Show();
        }
        function shen(OrderNumber)
        {
              location.href = '/Order/ShowOrderALL?OrderNumber='+ OrderNumber;
           
       
      
        }
    </script>
